<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
<a href="/dashboard">Home</a>
<a href="/students">View Students</a>
<table class="table table-inverse">
      <thead>
        <tr>
          <th>Id</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
          <th>Phone</th>
          <th colspan="2">Action</th>
        </tr>
      </thead>

      <tbody>
        <% for(var i=0; i < studentsList.length; i++) { %>
       <tr>

         <td><%= studentsList[i].id %></td>
         <td><%= studentsList[i].first_name %></td>
         <td><%= studentsList[i].last_name %></td>
         <td><%= studentsList[i].email %></td>
         <td><%= studentsList[i].phone %></td>
         <td><a href="/editForm/<%= studentsList[i].id %>">Edit</a></td>
         <td><a href="/student/<%= studentsList[i].id%>?_method=DELETE">Delete</a></td>
       </tr>
    <% } %>
      </tbody>

    </table>
</body>
</html>